{
  "name": "node-ios-device",
  "version": "2.0.0",
  "description": "Simple library for detecting and installing apps on iOS devices",
  "main": "./ios-device",
  "author": "Axway, Inc. <npmjs@appcelerator.com>",
  "maintainers": [
    "Chris Barber <cbarber@axway.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "appcelerator",
    "ios",
    "iphone",
    "ipad",
    "install",
    "mobiledevice",
    "mobile",
    "device",
    "devices"
  ],
  "scripts": {
    "install": "node ./bin/run.js node-pre-gyp install --fallback-to-build",
    "build-n-publish-binding": "node ./bin/run.js node-pre-gyp rebuild package publish",
    "prepare": "node ./bin/run.js node ./bin/build-all.js",
    "rebuild": "node ./bin/run.js node-pre-gyp rebuild",
    "rebuild-debug": "node ./bin/run.js node-pre-gyp --debug rebuild",
    "xcode": "node ./bin/run.js node-pre-gyp configure -- -f xcode",
    "download-all": "node ./bin/download-all.js",
    "test": "exit 0"
  },
  "dependencies": {
    "nan": "^2.13.2",
    "node-pre-gyp": "^0.12.0",
    "node-pre-gyp-init": "^1.1.0",
    "snooplogg": "^2.0.1"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "aws-sdk": "^2.441.0"
  },
  "homepage": "https://github.com/appcelerator/node-ios-device",
  "bugs": "https://github.com/appcelerator/node-ios-device/issues",
  "repository": "https://github.com/appcelerator/node-ios-device",
  "binary": {
    "module_name": "node_ios_device",
    "module_path": "./binding/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{name}/v{version}",
    "host": "https://appc-node-binaries.s3.amazonaws.com",
    "targets": {
      "8.16.0": 57,
      "9.11.2": 59,
      "10.15.3": 64,
      "11.14.0": 67
    }
  },
  "gypfile": true,
  "engines": {
    "node": ">=8.0.0"
  }
}
